<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.projetjava.controller.StudentTimetableController"
      alignment="TOP_CENTER" spacing="10.0" prefHeight="600.0" prefWidth="800.0">
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>

    <Label fx:id="welcomeLabel" text="Emploi du Temps">
        <font>
            <Font name="System Bold" size="24.0"/>
        </font>
    </Label>

    <ListView fx:id="timetableListView" prefHeight="500.0" VBox.vgrow="ALWAYS"/> </VBox>